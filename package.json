{
	"name": "openresty",
	"displayName": "OpenResty Lua 代码补全",
	"publisher": "Killsen",
	"icon": "images/icon.png",
	"description": "",
	"license": "MIT",
	"private": true,
	"version": "0.4.3",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:lua"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "openresty.debug",
				"title": "openresty.debug"
			},
			{
				"command": "openresty.action",
				"title": "openresty.action"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js",
		"package": "yarn run vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/luaparse": "^0.2.6",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vsce": "^2.7.0",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"luaparse": "^0.3.0"
	}
}
